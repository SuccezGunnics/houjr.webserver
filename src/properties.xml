<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<default-port>2016</default-port>
	<max-conNumber>100</max-conNumber>
	<handler>
		<handler-class>com.succez.server.app.FileBrowsingHandler</handler-class>
	</handler>
	<default-root>d:</default-root>
	<browsable>
		<image>
			<jpg>.JPG</jpg>
			<gif>.GIF</gif>
			<png>.PNG</png>
		</image>
		<text>
			<txt>.TXT</txt>
			<java>.JAVA</java>
			<js>.JS</js>
			<html>.HTML</html>
			<xml>.XML</xml>
		</text>
	</browsable>
</web-app>